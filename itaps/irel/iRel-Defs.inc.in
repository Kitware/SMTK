IREL_CXXFLAGS = @CXXFLAGS@ 
IREL_CFLAGS = @CFLAGS@
IREL_LDFLAGS = @EXPORT_LDFLAGS@

IREL_FC = @FC@
IREL_FCFLAGS = @FCFLAGS@

IREL_FCDEFS = @DEFS@

IREL_CPPFLAGS = -I$(IREL_INCLUDEDIR) $(IREL_INCLUDEDIR2)

# This definition is deprecated; use IREL_CPPFLAGS instead
IREL_INCLUDES = $(IREL_CPPFLAGS)

IREL_LIBS = $(IREL_LDFLAGS) -L${MOAB_LIBDIR} -L$(IREL_LIBDIR) \
              -liRel -liMesh $(FBIGEOM_LIBS) -lMOAB \
              @NETCDF_LIBS@ @PNETCDF_LIBS@ @HDF5_LIBS@ @DAMSEL_LIBS@ \
              @CGM_LDFLAGS@ @CGM_LIBS@ @CCMIO_LIBS@ @ZOLTAN_LIBS@ @PARMETIS_LIBS@ @METIS_LIBS@ \
              @FCLIBS@ @LIBS@


IREL_INCLUDEDIR = @abs_srcdir@
IREL_INCLUDEDIR2 = -I@abs_top_builddir@
IREL_LIBDIR = @abs_builddir@/.libs
MOAB_LIBDIR = @abs_builddir@/../../.libs

